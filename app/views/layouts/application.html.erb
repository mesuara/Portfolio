<!DOCTYPE html>
<html>
  <head>
    <title>Portfolio</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/1.11.8/semantic.min.css"/>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/numeral.js/2.0.6/numeral.min.js"></script>
  
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/1.11.8/semantic.min.js"></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
     <div id="maindiv">
    <div><h2>Welcome To My Portfolio</h2></div>
    <div id="maincontent">
        <div class="image">
        <img id="me" src="/assets/me.jpg" />
        </div>
        <div id="title">
        <div id="name">
        <h2>Mesuara Kaleziq</h2>
        <button class="ui button">Contact</button>
        </div>
        <div id="follow">
            <h3>Projects</h3>
            <h5 id="projects">...</h5>
            <h3>Experience</h3>
            <h5 id="experience">...</h5>
        </div>
       </div>
    </div>

    <div id="categories">
        <a class="linksmain" id="about" href="/abouts">About</a>
       <a class="linksmain" id="pro" href="/projects">Projects</a>
    </div>

    <div id="info">
      
    </div>

      <%= yield %>

       <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/4.0.11/handlebars.min.js"></script>
   <script src="script.js"></script> 
  </body>
</html>
